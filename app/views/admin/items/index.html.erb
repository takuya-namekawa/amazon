 <%= link_to "新規登録" ,new_admin_item_path %>
<%= link_to "ジャンル一覧" , admin_genres_path %>

 <table class="table  table-light  ">
       <tr>
         <thed>
           <tr>
             <th>商品ID</th>
             <th>商品名</th>
             <th>税抜価格</th>
             <th>ジャンル</th>
             <th colspan="3"></th>
         </thed>
         <tbody>

         <% @items.each do |item| %>
          <tr>
         <td><%= item.id %></td>
         <td><%= link_to item.name, admin_item_path(item) %></td>
         <td><%= item.price %></td>
         <td><%= item.genre.name %></td>
         <td><%= item.is_ordered ? "販売中" : "販売停止中" %></td>
         </tr>
         <% end %>

         </tbody>
 </table>